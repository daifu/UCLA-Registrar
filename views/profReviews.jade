div(data-role='page', id='course_desc', data-dom-cache="true")
  header(data-role='header')
    a(href=ucla_back_link, data-icon='arrow-l', data-direction='reverse')
      = "Back"
    h1=ucla_prof+' ('+ucla_depart+')'
  div(data-role='content')
    ul(data-role='listview', data-inset='true')
      li(data-role='list-divider')
        h3='Average Reviews'
      li(data-role='fieldcontain')
        != partial('profAvgReviews')
      li(data-theme='b', data-icon='mynew-window', data-iconpos='right')
        a(href=ucla_bruin_walk_link, data-icon='arrow-r', data-iconpos='right')
          ='Click Here to BruinWalk'
  != partial('backlinks')
  != partial('footer')
